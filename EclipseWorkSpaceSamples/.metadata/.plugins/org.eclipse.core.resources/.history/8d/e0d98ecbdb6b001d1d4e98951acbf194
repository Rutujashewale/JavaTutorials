
public class Employee extends Person {
	private String Emp_id;
	
	Employee(String Emp_id,int age,String name,){
		
		super(age,name);
		this.Emp_id=Emp_id;
	}
	
	public String getEmpId() {
		return this.Emp_id;
	}
	public void setEmpId(String Emp_id) {
		this.Emp_id=Emp_id;
	}
	
	public String toString() {
		return this.age + " " + this.name + " " + this.Emp_id;
	}
}
